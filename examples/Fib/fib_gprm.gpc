// Fibonacci implementation in GPC

//C++ info, ignored by GPCC
#include "src/GPRM/Kernel/Fib.h"
#define seq
#define par

// Calculate nth Fibonacci number
int n = 10;

GPRM::Kernel::Fib f;

int get_fib(int n) {
    
    if (n >= 2) {
        int a = get_fib(n - 1);
        int b = get_fib(n - 2);
        return a + b;
    } else {
        return n;
    } 
}

void fib_gprm() {
    seq {
        f.start_timer();
        int a = get_fib(n);
        f.timer(a);
    }
}


