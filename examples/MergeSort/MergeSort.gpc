GPRM::Kernel::Mergesort MS;

int size = 20000000;

void MergeSort() {
    seq {
        MS.array(size);
        MS.merge_two(size, 
            MS.merge_two(size/2, MS.serial_ms(0, size/4), MS.serial_ms(5, size/4)),
            MS.merge_two(size/2, MS.serial_ms(10, size/4), MS.serial_ms(15, size/4))
        );  
    }
}
